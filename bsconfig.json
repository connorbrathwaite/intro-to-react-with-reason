{
  "name": "github-reason-playground",
  "namespace": true,
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors", "-bs-g"],
  "suffix": ".bs.js",
  "refmt": 3,
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "bs-dependencies": ["@glennsl/bs-json", "bs-fetch", "reason-react", "reason-react-update"]
}
